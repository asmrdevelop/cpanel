[%
	IF NOTICE.pkgfile;
		IF NOTICE.errors.size;
			CPANEL.locale.maketext('A full backup of the account “[_1]” with the primary domain “[_2]” has completed, but the upload failed to complete.', NOTICE.user, NOTICE.user_domain);
		ELSE;
			CPANEL.locale.maketext('A full backup of the account “[_1]” with the primary domain “[_2]” has completed.', NOTICE.user, NOTICE.user_domain);
		END;
	ELSE;
		CPANEL.locale.maketext('A full backup of the account “[_1]” with the primary domain “[_2]” failed to complete.', NOTICE.user, NOTICE.user_domain);
	END;
%]
