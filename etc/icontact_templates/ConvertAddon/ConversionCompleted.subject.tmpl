[%-
  PROCESS "include/icons.tmpl";
  IF NOTICE.is_success;
    get_icon(icon='success') _ " " _ CPANEL.locale.maketext('Conversion of Addon Domain “[_1]” into the [asis,cPanel] account “[_2]” succeeded', NOTICE.addon_domain, NOTICE.new_username);
  ELSE;
    get_icon(icon='error') _ " " _ CPANEL.locale.maketext('Conversion of Addon Domain “[_1]” into the [asis,cPanel] account “[_2]” failed', NOTICE.addon_domain, NOTICE.new_username);
  END;
-%]
