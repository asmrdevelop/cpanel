CREATE TABLE registrations (
    aps_device_token char(128),
    aps_account_id char(128),
    dovecot_username char(128),
    dovecot_mailbox char(255),
    register_time unsigned integer not null default '0',
    PRIMARY KEY(aps_device_token,aps_account_id,dovecot_username, dovecot_mailbox)
);

CREATE INDEX mailbox_username_index on registrations (dovecot_mailbox, dovecot_username);
