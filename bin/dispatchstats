use Cpanel::Dispatch::OverLoad::LogRunner ();

__PACKAGE__->script(@ARGV) if !caller();

sub script {
    my ( $class, $pid ) = @_;
    Cpanel::Dispatch::OverLoad::LogRunner->new( 'pid' => $pid )->dispatch();
}

1;
