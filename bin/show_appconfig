package scripts::show_appconfig;

use strict;

use Cpanel::AppConfig ();
use Cpanel::YAML      ();

run() unless caller;

sub run {

    print Cpanel::YAML::Dump( scalar Cpanel::AppConfig::get_application_list() );

    return;
}
