import sys

sys.path.insert(0, '/usr/local/cpanel/lib/python2')

import cPanel


def main(argv):
    sys.stdout.write(cPanel.json_to_pickle(sys.stdin.read()))


if __name__ == '__main__':
    raise SystemExit(main(sys.argv))
