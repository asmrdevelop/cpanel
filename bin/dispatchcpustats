use Cpanel::Dispatch::OverLoad::CpuWatch ();

__PACKAGE__->script(@ARGV) if !caller();

sub script {
    my ( $class, $pid ) = @_;
    Cpanel::Dispatch::OverLoad::CpuWatch->new( 'pid' => $pid )->dispatch();
}

1;
