# This code is subject to the cPanel license. Unauthorized copying is prohibited

use Cpanel::Features::Migrate ();

my ( $status, $modified ) = Cpanel::Features::Migrate::migrate_all_feature_lists_to_current();

if ($status) {
    print "Migrated $modified feature list(s)\n";
}
else {
    print "Failed to migrate feature lists\n";
}

exit 0;
