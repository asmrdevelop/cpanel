use strict;
use Cpanel::Gzip::Config ();

my $config  = Cpanel::Gzip::Config->load();
my @command = $config->command();

-x $config->{'bin'} or die("Binary ($config->{'bin'}) not available");

exec( @command, @ARGV ) or die("Failed to exec: @command: $!");
