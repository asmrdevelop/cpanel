<div id="loc_detail_{{ zone_rec._id }}">
    <span class="record-property-label">[% locale.maketext('Latitude:') %]  </span>{{ zone_rec.latitude }}<br>
    <span class="record-property-label">[% locale.maketext('Longitude:') %]  </span>{{ zone_rec.longitude }}<br>
    <span class="record-property-label">[% locale.maketext('Altitude:') %]  </span>{{ zone_rec.altitude }} [% locale.maketext('meters[comment,label,lowercase]') %]<br>
    <span class="record-property-label">[% locale.maketext('Size:') %] </span>{{ zone_rec.size }} [% locale.maketext('meters[comment,label,lowercase]') %]<br>
    <span class="record-property-label">[% locale.maketext('Horizontal:') %] </span>{{ zone_rec.horiz_pre }} [% locale.maketext('meters[comment,label,lowercase]') %]<br>
    <span class="record-property-label">[% locale.maketext('Vertical:') %]  </span>{{ zone_rec.vert_pre }} [% locale.maketext('meters[comment,label,lowercase]') %]<br>
</div>