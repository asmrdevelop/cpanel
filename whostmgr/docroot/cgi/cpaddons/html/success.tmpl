<div class="alert alert-success" role="alert">
    <span class="glyphicon glyphicon-ok-sign" aria-hidden="true"></span>
    <div class="alert-message[%IF classes%] [% classes %][%END%]" id="[% id | url %]">
        <strong>[% locale.maketext("Success:") %]</strong>
        [% message | html %]
    </div>
</div>
