<div class="modal-header">
    <h4><span class="confirm_message">[% locale.maketext('Confirm Revocation') %]</span></h4>
</div>
<div class="modal-body">
    {{ctrl.prompt}}
</div>
<div class="modal-footer">
    <button class="btn btn-primary btn-sm"
        cp-action="ctrl.confirm()">[% locale.maketext('Continue') %]</button>
    <button class="btn btn-link btn-sm" ng-click="ctrl.cancel()">[% locale.maketext('Cancel') %]</button>
</div>
