[% WRAPPER 'master_templates/master.tmpl' theme="yui"
	header = locale.maketext('Add a DNS Zone')
	breadcrumburl = '/scripts2/adddns'
-%]
[% USE Encoder -%]
<h2>Results of adding zone [% Encoder.safe_html_encode_str(data.domain) %]</h2>
[% IF data.status -%]
[% Encoder.safe_html_encode_str(data.owner_message) %]
<br />
Zone created successfully for IP [% data.ip %].
<br />
Zone is owned by [% data.owner %].
[% ELSE -%]
[% Encoder.safe_html_encode_str(data.message) %]
[% END -%]
[% END -%]
