<span class="label selection-status"
    ng-class="{ 'label-success': ngModel, 'label-danger': !ngModel }">
    <span class="glyphicon move-status-icon" ng-class="{ 'glyphicon-ok-sign': ngModel, 'glyphicon-remove-sign': !ngModel }"></span>
    <span class="move-status-text">{{ ngModel ? moveLabel : noMoveLabel }}</span>
</span>
