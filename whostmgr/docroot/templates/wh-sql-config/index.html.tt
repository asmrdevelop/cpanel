#----------------------------------------------------
#
# Build webpack css bundle list
style_sheets.push(Whostmgr.find_file_url(TEMPLATE_PATH _ "styles.6edeb3bd93b7f3dd.css"));

# Build webpack javascript bundle list
scripts.push(Whostmgr.find_file_url(TEMPLATE_PATH _ "runtime.4e347b7e7fa23e2e.js"));
scripts.push(Whostmgr.find_file_url(TEMPLATE_PATH _ "vendor.3e9b7c8f58292097.js"));
scripts.push(Whostmgr.find_file_url(TEMPLATE_PATH _ "polyfills.45915eb100557a4b.js"));
scripts.push(Whostmgr.find_file_url(TEMPLATE_PATH _ "main.0287e3be69796589.js"));

