"""Show basic statistics about, and build options for this
installation of Mailman. Requires python 2."""

print """Configuration and build information for Mailman\n"""

print 'Mailman version: %s' % '2.1.39'
print 'Build Date:      %s' % 'Tue Aug 30 22:32:15 UTC 2022'
print ''
print 'prefix:          %s' % '/usr/local/cpanel/3rdparty/mailman'
print 'var_prefix:      %s' % '/usr/local/cpanel/3rdparty/mailman'
print 'mailman_user:    %s' % 'mailman'
print 'mailman_group:   %s' % 'mailman'
print 'mail_group:      %s' % 'mail'
print 'cgi_group:       %s' % 'nobody'
print ''


print 'configure_opts: "%s"' % '--build=x86_64-linux-gnu --prefix=/usr --includedir=${prefix}/include --mandir=${prefix}/share/man --infodir=${prefix}/share/info --sysconfdir=/etc --localstatedir=/var --disable-option-checking --disable-silent-rules --libdir=${prefix}/lib/x86_64-linux-gnu --runstatedir=/run --disable-maintainer-mode --disable-dependency-tracking --without-permcheck --with-mail-gid=mail --with-cgi-gid=nobody --with-python=/usr/local/cpanel/3rdparty/bin/python2 --prefix=/usr/local/cpanel/3rdparty/mailman --with-mailhost=localhost.localdomain --with-urlhost=localhost.localdomain'

