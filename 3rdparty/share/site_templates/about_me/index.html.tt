<html>
    <head>
        [% IF fullName.length %]
        <title>[% fullName | html %]</title>
        [% END %]
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
        <link rel="stylesheet" href="assets/css/main.css" />
        <!--[if lte IE 8]><link rel="stylesheet" href="assets/css/ie8.css" /><![endif]-->
    </head>
    <body>

        <!-- Header -->
            <section id="header">
                [% IF avatarURL.length OR fullName.length OR tagLine.length %]
                <header>
                     [% IF avatarURL.length %]
                    <span class="image avatar">
                        <img src="[% avatarURL | url %]" alt="Avatar" />
                    </span>
                    [% END %]

                    [% IF fullName.length %]
                    <h1 id="logo">
                        <a href="./">[% fullName | html %]</a>
                    </h1>
                    [% END %]

                    [% IF tagLine.length %]
                        <p>[% tagLine | html %]</p>
                    [% END %]
                </header>
                [% END %]

                <nav id="nav">
                    <ul>
                        <li><a href="#one" class="active">About</a></li>
                         [% IF thingsIDo.length %]
                        <li><a href="#two">Things I Can Do</a></li>
                        [% END %]
                        [% IF accomplishments.length %]
                        <li><a href="#three">A Few Accomplishments</a></li>
                        [% END %]
                    </ul>
                </nav>

                <footer>
                    <ul class="icons">
                        [% IF emailAddress.length OR linkedinLink.length OR twitterLink.length OR facebookLink.length OR instagramLink.length OR githubLink.length %]
                        <ul class="icons">
                            [% IF emailAddress.length %]
                            <li><a href="mailto:[% emailAddress | uri %]" class="icon  fa-envelope" title="Email"><span class="label">Email</span></a></li>
                            [% END %]

                            [% IF twitterLink.length %]
                            <li><a href="[% twitterLink | url %]" class="icon  fa-twitter" target="_blank" title="Twitter"><span class="label">Twitter</span></a></li>
                            [% END %]

                            [% IF facebookLink.length %]
                            <li><a href="[% facebookLink | url %]" class="icon  fa-facebook" target="_blank" title="Facebook"><span class="label">Facebook</span></a></li>
                            [% END %]

                            [% IF linkedinLink.length %]
                            <li><a href="[% linkedinLink | url %]" class="icon  fa-linkedin" target="_blank"  title="LinkedIn"><span class="label">LinkedIn</span></a></li>
                            [% END %]

                            [% IF instagramLink.length %]
                            <li><a href="[% instagramLink | url %]" class="icon fa-instagram" target="_blank"  title="Instagram"><span class="label">Instagram</span></a></li>
                            [% END %]

                             [% IF githubLink.length %]
                            <li><a href="[% githubLink | url %]" class="icon  fa-github" target="_blank"  title="Github"><span class="label">Github</span></a></li>
                            [% END %]
                        </ul>
                        [% END %]
                    </ul>
                </footer>
            </section>

        <!-- Wrapper -->
            <div id="wrapper">

                <!-- Main -->
                    <div id="main">
                        <!-- One -->
                        <section id="one">
                            <div class="container">
                                [% IF aboutMeHeading.length || aboutMeTagLine.length %]
                                <header class="major">
                                    [% IF aboutMeHeading.length %]
                                    <h2>[% aboutMeHeading | html %]</h2>
                                    [% END %]

                                    [% IF aboutMeTagLine.length %]
                                    <p>[% aboutMeTagLine | html %]</p>
                                    [% END %]
                                </header>
                                [% END %]

                                [% IF aboutMe.length %]
                                <p>[% aboutMe | html %]</p>
                                [% END %]
                            </div>
                        </section>

                        <!-- Two -->
                        [% IF thingsIDo.length %]
                            <section id="two">
                                <div class="container">
                                    <h3>Things I Can Do</h3>
                                    <section>
                                    [% thingsIDo | html %]
                                    </section>
                                </div>
                            </section>
                        [% END %]

                        <!-- Three -->
                        [% IF accomplishments.length %]
                            <section id="three">
                                <div class="container">
                                    <h3>A Few Accomplishments</h3>
                                    <section>[% accomplishments | html %]</section>
                                </div>
                            </section>
                        [% END %]
                    </div>

                <!-- Footer -->
                    [% IF fullName.length %]
                    <section id="footer">
                        <div class="container">
                        <ul class="copyright">
                            <li>&copy; [% fullName | html %]</li>
                            <li>Design: <a href="//html5up.net">HTML5 UP</a></li>
                        </ul>
                        </div>
                    </section>
                    [% END %]
            </div>

        <!-- Scripts -->
            <script src="assets/js/jquery.min.js"></script>
            <script src="assets/js/jquery.scrollzer.min.js"></script>
            <script src="assets/js/jquery.scrolly.min.js"></script>
            <script src="assets/js/skel.min.js"></script>
            <script src="assets/js/util.js"></script>
            <!--[if lte IE 8]><script src="assets/js/ie/respond.min.js"></script><![endif]-->
            <script src="assets/js/main.js"></script>

            [% IF google_analytics_key.length %]
                <script type="text/javascript">

                  var _gaq = _gaq || [];
                  _gaq.push(['_setAccount', '[% google_analytics_key | html %]']);
                  _gaq.push(['_trackPageview']);

                  (function() {
                    var ga = document.createElement('script');
                    ga.type = 'text/javascript'; ga.async = true;
                    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                    var s = document.getElementsByTagName('script')[0];
                    s.parentNode.insertBefore(ga, s);
                  })();

                </script>
            [% END %]
    </body>
</html>
