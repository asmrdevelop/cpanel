{
    "name": "plesk/wp-cli-toolkit-command",
    "description": "",
    "type": "wp-cli-package",
    "homepage": "https://vbaranovskiy@git.plesk.ru/scm/psdk/wp-cli-toolkit-command.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Vlad Baranovskiy",
            "email": "vbaranovskiy@plesk.com",
            "homepage": "https://plesk.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": [ "toolkit-command.php" ]
    },
    "require": {
        "php": ">=5.4",
        "ext-json": "*",
        "wp-cli/cron-command": "^2.0",
        "wp-cli/extension-command": "^2.0",
        "wp-cli/rewrite-command": "^2.0",
        "wp-cli/wp-cli": "*"
    },
    "require-dev": {
        "behat/behat": "~2.5"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "5.4"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "bundled": true,
        "commands": [
            "admin-username",
            "config-settings",
            "db-prefix",
            "instance",
            "maintenance-mode",
            "release",
            "security-keys",
            "version-info"
        ]
    }
}
