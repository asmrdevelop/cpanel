<li class="validation validation-error ng-hide" ng-show="canShow()">
    <i class="fas fa-exclamation-circle" ng-show="showIcon()"></i>
    <span id="{{id}}" class="validation-message">
        <span ng-show="!showIcon()" ng-class="[ prefixClass ]"></span>{{print()}}<span ng-transclude></span>
    </span>
</li>