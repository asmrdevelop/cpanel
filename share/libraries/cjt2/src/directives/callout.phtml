<div class="callout callout-{{calloutType}}" aria-label="{{calloutType}}">
    <button type='button' class='close'
        ng-if='closeable'
        ng-click='runClose()'
        aria-label="Close">
            <span aria-hidden="true">&times;</span>
    </button>
    <div class="callout-heading" ng-show="hasHeading">{{calloutHeading}}</div>
    <div ng-transclude></div>
</div>