        );
    }

    if ( $self->get('path') ) {
        return Cpanel::LocaleString->new(
            'The system failed to read from [_1] because of an error: [_2]',
            $self->get('path'),
            $self->get('error'),
        );
    }

    return Cpanel::LocaleString->new(
        'The system failed to read from a file handle because of an error: [_1]',
        $self->get('error'),
    );
}

1;

} # --- END Cpanel/Exception/IO/ReadError.pm


