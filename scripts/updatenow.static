{ # --- BEGIN Whostmgr/Transfers/State.pm
package Whostmgr::Transfers::State;


use strict;

my $is_transfer = 0;

sub start_transfer {
    $is_transfer = 1;
    return 1;
}

sub is_transfer {
    return ( $is_transfer eq 1 ) ? 1 : 0;
}

sub end_transfer {
    $is_transfer = 0;
    return 1;
}

1;

} # --- END Whostmgr/Transfers/State.pm


