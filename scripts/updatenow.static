    $self->{'_trans'} = $trans_class->new( path => $_datastore_file );

    $self->{'_data'} = $self->{'_trans'}->get_data();

    if ( 'SCALAR' eq ref $self->{'_data'} ) {
        $self->{'_data'} = {};

        $self->_init_data_from_legacy();
