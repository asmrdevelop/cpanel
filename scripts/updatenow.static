package Cpanel::IO;


use strict;

sub read_bytes_to_end_of_line {    ##no critic qw(RequireArgUnpacking)
    my $buffer;
    if ( read( $_[0], $buffer, $_[1] || 32768 ) ) {
        my $next = readline( $_[0] );
        $next = '' unless defined $next;
        return $buffer . $next;
    }

    return undef;
}

