sub get_data_struct_from_legacy {
    my %legacy;

    if ( -s $_legacy_iclevels_file ) {
        Cpanel::FileUtils::Read::for_each_line(
            $_legacy_iclevels_file,
            sub {
                chomp;
                s/\r//g;
                if (m/^(\S+)\s+(\S+)/) {
                    my $app   = $1;
                    my $level = $2;
                    next if !length $level;

                    $legacy{$app} = $level;
                }
            },
        );
    }

    return \%legacy;
}

