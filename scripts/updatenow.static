# use Cpanel::Data::Result();
BEGIN { push @ISA, qw(Cpanel::Data::Result); }

BEGIN {

    no warnings 'once';

    *error = *Cpanel::Data::Result::Success::get;
}

sub get ($self) {
    return Cpanel::Data::Result::_carp( $$self // '' );
}

